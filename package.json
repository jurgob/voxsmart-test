{
  "name": "voxsmart-test",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts | pino-pretty",
    "dev:logs": "tail -f /tmp/voxtest.log | pino-pretty",
    "dev:logs:clean": "echo '' > /tmp/voxtest.log",
    "build": "tsc",
    "_test": "LOG_ON_FILE=true node --no-warnings --import tsx --test $@",
    "test": "nyc npm run _test -- src/*test.ts",
    "test:w": "npm run _test -- --watch src/*test.ts",
    "test:file": "npm run _test -- $1",
    "test:file:w": "npm run _test -- --watch $1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.28",
    "cjs-mock": "^1.0.0",
    "nyc": "^15.1.0",
    "pino-pretty": "^10.3.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "express": "^4.18.3",
    "pino": "^8.19.0"
  }
}
